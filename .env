# Your Helius RPC endpoint or any Solana RPC
RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY

# Tunables (change as needed)
POLL_INTERVAL_MS=10000         # 10s block polling
WINDOW_MS=180000               # 3 min window to detect "fan-out"
MIN_CHILDREN=5                 # parent must fund >=5 unique wallets
SPEND_REFRESH_MS=15000         # 15s balance refresh for active clusters
SPEND_RATE_WINDOW_MS=60000     # 60s window to compute spend rate
